ChallengeResponseAuthentication no
Compression yes
CompressionLevel 9
ForwardAgent yes
GSSAPIAuthentication no
KbdInteractiveAuthentication no
PasswordAuthentication no
PubkeyAuthentication yes
RSAAuthentication no
RequestTTY auto
RhostsRSAAuthentication no
StrictHostKeyChecking no

Host app-vagrant
  Hostname localhost
  User vagrant
  Port 2222
  IdentityFile ~/.vagrant.d/insecure_private_key
  IdentityFile ./secrets/server_ssh_key
  LogLevel=quiet
  UserKnownHostsFile=/dev/null
  StrictHostKeyChecking=no

Host es1-vagrant
  Hostname localhost
  User vagrant
  Port 2200
  IdentityFile ~/.vagrant.d/insecure_private_key
  IdentityFile ./secrets/server_ssh_key
  LogLevel=quiet
  UserKnownHostsFile=/dev/null
  StrictHostKeyChecking=no

Host es2-vagrant
  Hostname localhost
  User vagrant
  Port 2201
  IdentityFile ~/.vagrant.d/insecure_private_key
  IdentityFile ./secrets/server_ssh_key
  LogLevel=quiet
  UserKnownHostsFile=/dev/null
  StrictHostKeyChecking=no

